pipeline {
    agent any 

    environment {
        ENV_URL = "plepline.google.com"
    } 

    stages {
        stage('Hai') {
            steps { 
                sh "echo ${ENV_URL}"
             }
         }  

        stage('Hai') {
            steps {
                sh  "echo hai"
                sh  "echo Environment  URL is ${ENV_URL} "
            }
        } 
    }   
}